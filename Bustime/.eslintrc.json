{
 "env": {
    "browser": true,
    "es6": true
  },
 
 "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 6
   },
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],

    "rules": {
        "no-alert": 0,
        //表示在Windows下默认的换行回车符，CRLF  \r\n
        "linebreak-style":[1,"windows"]
    }
}